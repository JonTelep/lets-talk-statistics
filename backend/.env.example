# Let's Talk Statistics - Backend Configuration
# Copy to .env and fill in values

# App settings
DEBUG=false

# API Keys (optional - most gov APIs work without keys)
# Get keys for higher rate limits:
# - Census: https://api.census.gov/data/key_signup.html
# - BLS: https://data.bls.gov/registrationEngine/
# - FEC: https://api.open.fec.gov/developers/

CENSUS_API_KEY=
BLS_API_KEY=
FEC_API_KEY=
FRED_API_KEY=

# Housing Postgres database
HOUSING_DB_HOST=localhost
HOUSING_DB_PORT=5432
HOUSING_DB_USER=admin
HOUSING_DB_PASSWORD=
HOUSING_DB_NAME=housing_db

# Data cache settings
DATA_DIR=/app/data
CACHE_TTL_HOURS=24

# CORS (JSON array format)
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
